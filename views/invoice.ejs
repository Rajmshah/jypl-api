            margin: 0 auto;
        }

        .row {
            margin: 15px -15px 15px -15px;
        }

        .row:before,
        .row:after {
            content: " ";
            display: table;
        }

        .row:after {
            clear: both;
        }

        .col-xs-2 {
            position: relative;
            min-height: 1px;
            padding-left: 15px;
            padding-right: 15px;
            float: left;
            width: 16.66667%;
        }

        .col-xs-4 {
            position: relative;
