            position: relative;
            min-height: 1px;
            padding-left: 15px;
            padding-right: 15px;
            float: right;
            width: 66.66667%;
        }

        table {
            background-color: transparent;
        }

        th {
            text-align: left;
        }

        .table {
            width: 100%;
            max-width: 100%;
            margin-bottom: 20px;
            border-collapse: separate;
            border: 1px solid rgb(165, 147, 147);
        }

        .table>thead>tr>th,
        .table>thead>tr>td,
        .table>tbody>tr>th,
        .table>tbody>tr>td {
            border: 0;
            padding: 10px;
            word-break: break-all;
        }

